  "dependencies": {
    "@fastify/middie": "^8.0.0",
    "@fastify/static": "^6.5.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitely/core": "^0.1.0",
    "fastify": "^4.5.2"
  },
  "devDependencies": {
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vue": "^3.2.37",
    "vue-router": "^4.1.4"
  },
  "peerDependencies": {
    "vite": "^2 || ^3",
    "vue": "^3",
    "vue-router": "^4"
