{
  "name": "@vitely/vue",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./router/runtime": {
      "import": "./dist/router/runtime/index.mjs",
      "types": "./dist/router/runtime/index.d.mts"
