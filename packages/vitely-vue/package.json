{
  "name": "@vitely/vue",
  "version": "0.0.6",
  "repository": {
    "type": "git",
    "url": "https://github.com/rhangai/vitely/tree/main/packages/vitely-vue"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./router/runtime": {
      "import": "./dist/router/runtime/index.mjs",
      "types": "./dist/router/runtime/index.d.mts"
    },
    "./composition": {
      "import": "./dist/composition/index.mjs",
      "types": "./dist/composition/index.d.mts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist node_modules .turbo",
    "build": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitely/core": "^0.0.6",
    "@vueuse/head": "^0.7.9"
  },
  "devDependencies": {
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vue": "^3.2.37",
    "vue-router": "^4.1.4"
  },
  "peerDependencies": {
    "vite": "^2 || ^3",
    "vue": "^3",
    "vue-router": "^4"
  }
}
