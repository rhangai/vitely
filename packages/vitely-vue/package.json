    "./runtime/entry": {
      "import": "./dist/runtime/index.mjs",
      "types": "./dist/runtime/index.d.mts"
    },
    "./runtime/spa/client": {
      "import": "./dist/runtime/spa/client.mjs",
      "types": "./dist/runtime/spa/client.d.mts"
    },
    "./runtime/spa/server": {
      "import": "./dist/runtime/spa/server.mjs",
      "types": "./dist/runtime/spa/server.d.mts"
    },
    "./runtime/ssr/client": {
      "import": "./dist/runtime/ssr/client.mjs",
      "types": "./dist/runtime/ssr/client.d.mts"
    },
    "./runtime/ssr/server": {
      "import": "./dist/runtime/ssr/server.mjs",
      "types": "./dist/runtime/ssr/server.d.mts"
    },
    "./runtime/ssr/server-render": {
      "import": "./dist/runtime/ssr/server-render.mjs",
