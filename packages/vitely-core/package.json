{
  "name": "@vitely/core",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist node_modules .turbo",
    "build": "tsc -p tsconfig.build.json && tsc -p tsconfig.build-cjs.json && echo '{\"type\":\"module\"}' > dist/esm/package.json"
  },
  "dependencies": {
    "tapable": "^2.2.1",
    "vite": "^3.0.9"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
    "typescript": "^4.7.4",
    "vitest": "^0.22.1"
  }
}
