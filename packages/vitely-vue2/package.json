{
  "name": "@vitely/vue2",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./router/runtime": {
      "import": "./dist/router/runtime/index.mjs",
      "types": "./dist/router/runtime/index.d.mts"
    },
    "./composition": {
      "import": "./dist/composition/index.mjs",
      "types": "./dist/composition/index.d.mts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist node_modules .turbo",
    "build": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vitely/core": "^0.0.1",
    "vue-meta": "^2.4.0",
    "vue-router": "^3"
  },
  "devDependencies": {
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vue": "^2.7",
    "vue-server-renderer": "^2.7.10"
  },
  "peerDependencies": {
    "vite": "^2 || ^3",
    "vue": "^2"
  }
}
