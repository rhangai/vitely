{
  "name": "@vitely/vite-plugin-vue-router",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./runtime": {
      "require": "./dist/cjs/runtime/index.js",
      "import": "./dist/esm/runtime/index.js",
      "types": "./dist/esm/runtime/index.d.ts"
    }
  },
  "keywords": [
    "vite-plugin"
  ],
  "scripts": {
    "clean": "rm -rf dist node_modules .turbo",
    "build": "tsc -p tsconfig.build.json && tsc -p tsconfig.build-cjs.json && echo '{\"type\":\"module\"}' > dist/esm/package.json"
  },
  "dependencies": {
    "@vitely/core-runtime": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^4.7.4",
    "vite": "^3",
    "vue": "^3.2.25",
    "vue-router": "^4"
  },
  "peerDependencies": {
    "vite": "^2 || ^3",
    "vue-router": "^4"
  }
}
