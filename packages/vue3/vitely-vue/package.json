{
  "name": "@vitely/vue",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist node_modules .turbo",
    "build": "tsc -p tsconfig.build.json && tsc -p tsconfig.build-cjs.json && echo '{\"type\":\"module\"}' > dist/esm/package.json",
    "dev": "node dist/index.js"
  },
  "dependencies": {
    "@fastify/middie": "^8.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitely/core": "workspace:*",
    "@vitely/vite-plugin-vue-router": "workspace:*",
    "@vitely/vue-runtime": "workspace:*",
    "fastify": "^4.5.2",
    "vite": "^3.0.9"
  },
  "devDependencies": {
    "typescript": "^4.7.4",
    "vue": "^3.2.37"
  }
}
